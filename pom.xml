        <spring.version>4.2.0.RELEASE</spring.version>
        <!-- junit version -->
        <junit.version>4.4</junit.version>
        <!-- mybatis version -->
        <mybatis.version>3.2.7</mybatis.version>
        <!-- data source version,datasource used c3p0 -->
        <c3p0.version>0.9.1.2</c3p0.version>
        <!-- jackson json parser -->
